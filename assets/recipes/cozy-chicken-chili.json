{
  "id": "cozy-chicken-chili",
  "title": "Cozy Chicken Chili",
  "metadata": {
    "imageUrl": "images/cozy-chicken-chili.jpg",
    "yields": "4 servings",
    "totalTime": "60 minutes",
    "prepTime": "20 minutes",
    "activeTime": "15 minutes",
    "handsOffTime": "20 minutes"
  },
  "preparationSteps": [
    {
      "id": "prep-step-1",
      "title": "Prep Step 1",
      "mainStep": "Prepare the first part of the cooking. Ingredients, they will be added in stages",
      "bullets": [
        "Measure 2 Tablespoons of olive oil",
        "Chop 1 large yellow onion",
        "Remove seeds and ribs from 1 jalapeño pepper, and finely chop"
      ]
    },
    {
      "id": "prep-step-2",
      "title": "Prep Step 2",
      "mainStep": "Make the Seasoning Mix. Mix in a small bowl",
      "bullets": [
        "Mince 5 garlic cloves",
        "1 teaspoon dried oregano",
        "1 teaspoon ground cumin",
        "½ teaspoon paprika or chili powder",
        "Pinch of cayenne pepper",
        "1 teaspoon kosher salt",
        "Few grinds of black pepper"
      ]
    },
    {
      "id": "prep-step-3",
      "title": "Prep Step 3",
      "mainStep": "Prepare and gather the second part of the cooking ingredients; they will all be added together",
      "bullets": [
        "Measure 4 cups chicken broth",
        "Open, rinse, and drain 2 cans of cannellini beans",
        "Open 2 cans of diced green chiles, and keep the liquid"
      ]
    },
    {
      "id": "prep-step-4",
      "title": "Prep Step 4",
      "mainStep": "Prepare Final Ingredients",
      "bullets": [
        "Shred 2½-3 cups of chicken from a rotisserie chicken",
        "Cut 1 lime in half",
        "Measure 1 cup corn kernels"
      ]
    },
    {
      "id": "prep-step-5",
      "title": "Prep Step 5",
      "mainStep": "Gather Optional Toppings",
      "bullets": [
        "Chopped avocado",
        "Pickled red onion",
        "Grated Cheddar",
        "Pickled jalapeños",
        "Sour cream",
        "Tortilla chips",
        "Lime wedges",
        "Chopped Cilantro"
      ]
    }
  ],
  "cookingSteps": [
    {
      "id": "cooking-step-1",
      "title": "Cooking Step 1",
      "mainStep": "Heat a large pot over medium heat",
      "bullets": [
        "Add and heat 2 Tablespoons of olive oil (1 minute)",
        "Add onion and jalapeno and cook onions until golden, stirring occasionally (5 minutes)"
      ]
    },
    {
      "id": "cooking-step-2",
      "title": "Cooking Step 2",
      "mainStep": "Keep temperature at medium heat",
      "bullets": [
        "Add the seasoning mix, and cook until fragrant (30 seconds)"
      ]
    },
    {
      "id": "cooking-step-3",
      "title": "Cooking Step 3",
      "mainStep": "Increase heat to medium high heat",
      "bullets": [
        "Add the 4 cups of chicken broth, 2 cans of drained cannellini beans, and 2 cans of diced green chiles"
      ]
    },
    {
      "id": "cooking-step-4",
      "title": "Cooking Step 4",
      "mainStep": "Bring to a boil, then reduce to medium-low. Simmer the soup, stirring occasionally (20 minutes)",
      "bullets": []
    },
    {
      "id": "cooking-step-5",
      "title": "Cooking Step 5",
      "mainStep": "Turn off heat",
      "bullets": [
        "Use a wooden spoon to mash some beans until the broth is thicker"
      ]
    },
    {
      "id": "cooking-step-6",
      "title": "Cooking Step 6",
      "mainStep": "Return pot to medium heat",
      "bullets": [
        "Stir in the 2 1/2 cups of shredded chicken and 1 cup of frozen corn",
        "Cook soup until heated through (3 minutes)"
      ]
    },
    {
      "id": "cooking-step-7",
      "title": "Cooking Step 7",
      "mainStep": "Serving",
      "bullets": [
        "Squeeze juice of half a lime",
        "Taste and adjust seasonings as needed",
        "Ladle into bowls and add desired toppings"
      ]
    }
  ],
